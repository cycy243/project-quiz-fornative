<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Quiz.ForNative.Components.Form.PictureInput"
             xmlns:local="clr-namespace:Quiz.ForNative.Components.Form"
             x:Name="pictureInput">
    <local:BaseInput LabelText="{Binding LabelContent, Source={x:Reference pictureInput}}">
        <Label Text="My file input" x:Name="Value" WidthRequest="250"/>
    </local:BaseInput>
</ContentView>
