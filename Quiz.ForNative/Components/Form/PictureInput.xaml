<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Quiz.ForNative.Components.Form.PictureInput"
             xmlns:local="clr-namespace:Quiz.ForNative.Components.Form"
             x:Name="pictureInput">
    <local:BaseInput LabelText="{Binding LabelContent, Source={x:Reference pictureInput}}">
        <Button x:Name="UploadBtn" Text="{Binding PlaceholderContent, Source={x:Reference pictureInput}}" WidthRequest="250" Clicked="UploadBtn_Clicked"/>
    </local:BaseInput>
</ContentView>
